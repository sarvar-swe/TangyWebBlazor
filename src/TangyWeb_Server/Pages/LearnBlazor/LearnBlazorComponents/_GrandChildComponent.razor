<div class="row">
    <h3 class="text-primary pl-4 pt-2 col-12">Grand Child Component</h3>
    <p>There is a message - @MessageForGrandChild</p>
    <p>Lucky number - @LuckyNumberForGrandChild</p>
</div>

@code{
    [CascadingParameter(Name="message")]
    public string? MessageForGrandChild { get; set; }
    [CascadingParameter(Name="luckyNumber")]
    public string? LuckyNumberForGrandChild { get; set; }
}